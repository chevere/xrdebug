{
    "api": "xr",
    "name": "XR Debug API",
    "version": "1.0.0",
    "servers": [
        {
            "url": "http:\/\/0.0.0.0:27420",
            "description": "XR Debug Server"
        }
    ],
    "paths": {
        "\/message": {
            "name": "\/message",
            "regex": "\/message",
            "endpoints": {
                "POST": {
                    "description": "Send a debug message",
                    "body": {
                        "type": "array#map",
                        "parameters": {
                            "body": {
                                "required": false,
                                "type": "string",
                                "regex": ".*?"
                            },
                            "emote": {
                                "required": false,
                                "type": "string"
                            },
                            "file_line": {
                                "required": false,
                                "type": "string"
                            },
                            "file_path": {
                                "required": false,
                                "type": "string"
                            },
                            "id": {
                                "required": false,
                                "type": "string"
                            },
                            "topic": {
                                "required": false,
                                "type": "string"
                            }
                        }
                    },
                    "responses": {
                        "200": [
                            {
                                "context": "MessagePostController",
                                "body": {
                                    "type": "null"
                                }
                            }
                        ]
                    }
                }
            }
        }
    }
}